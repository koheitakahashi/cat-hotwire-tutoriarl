<%= turbo_frame_tag cat do %>
  <%= bootstrap_form_with(model: cat, data: { action: "turbo:submit-end->modal#close"}) do |form| %>
    <%= form.text_field :name %>
    <%= form.number_field :age %>
    <%= form.primary %>
    <%= link_to "キャンセル", cat, class: "btn btn-sm btn-outline-secondary" %>
  <% end %>
<% end %>
